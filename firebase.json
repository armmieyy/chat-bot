{
  "hosting": {
    "public": "out",
    "ignore": ["**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "destination": "/Reply/[slug].html",
        "regex": "^/Reply/([^/]+?)(?:/)?$"
      },
      {
        "destination": "/Reply/[slug].html",
        "regex": "^/reply/([^/]+?)(?:/)?$"
      },
      {
        "destination": "thankyou.html",
        "regex": "^/thankyou"
      }
    ]
  },
  "functions": {
    "source": "functions"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": false
    }
  }
}
